{
  "name": "heaven-interview",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/empty-module.js",
      "^.+\\.css$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTest.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "ios.js"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.3",
    "es2015": "0.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "react-addons-test-utils": "^15.6.2",
    "react-scripts": "0.9.5",
    "react-test-renderer": "^15.6.2",
    "sinon": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eject": "react-scripts eject"
  }
}
